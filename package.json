{
  "name": "recipes-next",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "analyze": "npm run analyze -w client",
    "build": "npm run build -w client",
    "dev": "npm run typegen:local -w client && npm run dev -w client",
    "deploy": "npm run deploy -workspaces",
    "deploy:shared": "npm run deploy:shared -w cdk",
    "invalidate": "npm run invalidate -w client",
    "lint": "npm run lint --workspaces",
    "lint:fix": "npm run lint:fix --workspaces",
    "prepare": "husky install",
    "remove": "npm run destroy -w cdk && npm run remove -w client",
    "start": "npm run start -w client",
    "test": "npm run test --workspaces",
    "test:ci": "npm run test:ci --workspaces",
    "test:dev": "npm run test:dev --workspaces",
    "typecheck": "npm run typecheck --workspaces",
    "typegen": "npm run typegen -w client",
    "typegen:local": "npm run typegen:local -w client",
    "typegen:watch": "npm run typegen:watch -w client",
    "scan": "npm run scan -workspaces",
    "scan:local": "npm run scan:local -workspaces"
  },
  "workspaces": [
    "cdk",
    "client"
  ],
  "devDependencies": {
    "@tsconfig/recommended": "1.0.2",
    "@types/jest": "29.4.0",
    "@types/node": "18.14.2",
    "@typescript-eslint/eslint-plugin": "5.48.2",
    "@typescript-eslint/parser": "5.48.2",
    "cross-var-no-babel": "1.2.0",
    "dotenv": "16.0.3",
    "dotenv-cli": "7.0.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "husky": "^8.0.0",
    "jest": "29.4.3",
    "jest-environment-jsdom": "29.4.1",
    "lint-staged": "13.1.2",
    "prettier": "2.8.4",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tslint-config-prettier": "1.18.0",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.321.1",
    "@contentful/rich-text-plain-text-renderer": "^16.0.3",
    "algoliasearch": "^4.15.0",
    "amazon-cognito-identity-js": "^6.2.0",
    "contentful": "^9.3.5"
  }
}
