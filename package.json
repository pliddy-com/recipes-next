{
  "name": "recipes-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run typegen && next dev",
    "build": "npm run typegen && next build",
    "start": "npm run build && next start",
    "lint": "prettier --check . && next lint",
    "lint:fix": "prettier --write . && next lint --fix",
    "typecheck": "tsc",
    "typegen": "graphql-codegen --config codegen.yml --require dotenv/config",
    "typegen:watch": "graphql-codegen --watch --config codegen.yml --require dotenv/config"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^15.16.2",
    "@emotion/cache": "^11.10.5",
    "@emotion/react": "^11.10.5",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.5",
    "@graphql-codegen/typed-document-node": "^2.3.12",
    "@graphql-typed-document-node/core": "^3.1.1",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.5",
    "@mui/system": "^5.11.5",
    "@mui/utils": "^5.11.2",
    "@next/font": "13.1.2",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "eslint": "8.32.0",
    "eslint-config-next": "13.1.2",
    "eslint-config-prettier": "^8.6.0",
    "next": "13.1.2",
    "prettier": "^2.8.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "4.9.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.16.4",
    "@graphql-codegen/typescript": "^2.8.7",
    "@graphql-codegen/typescript-operations": "^2.5.12",
    "@tsconfig/recommended": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "tslint-config-prettier": "^1.18.0",
    "urql": "^3.0.3"
  }
}
