{
  "name": "recipes-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prebuild": "npm run typegen && npm run test",
    "build": "next build",
    "postbuild": "npm run sitemap",
    "sitemap": "next-sitemap",
    "predeploy": "npm run export",
    "deploy": "dotenv -- cross-var aws s3 sync --acl public-read out/ %S3_BUCKET%",
    "postdeploy": "dotenv -- cross-var aws cloudfront create-invalidation --distribution-id %DISTRIBUTION_ID% --paths '/*' --no-cli-pager",
    "predev": "npm run typegen",
    "dev": "next dev",
    "preexport": "npm run build",
    "export": "next export",
    "lint": "prettier --check . && next lint",
    "lint:fix": "prettier --write . && next lint --fix",
    "prepare": "husky install",
    "prestart": "npm run build",
    "start": "next start",
    "test": "jest",
    "test:dev": "jest --watch",
    "test:snapshot": "npm test -- -u",
    "test:ci": "jest --ci",
    "typecheck": "tsc",
    "typegen": "graphql-codegen --config codegen.yml --require dotenv/config",
    "typegen:watch": "graphql-codegen --watch --config codegen.yml --require dotenv/config"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^15.16.2",
    "@emotion/cache": "^11.10.5",
    "@emotion/react": "^11.10.5",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.5",
    "@graphql-codegen/typed-document-node": "^2.3.12",
    "@graphql-typed-document-node/core": "^3.1.1",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.5",
    "@mui/system": "^5.11.5",
    "@mui/utils": "^5.11.2",
    "@next/font": "13.1.2",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint": "8.32.0",
    "eslint-config-next": "13.1.2",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "next": "13.1.2",
    "prettier": "^2.8.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "4.9.4"
  },
  "devDependencies": {
    "@contentful/rich-text-types": "^16.0.2",
    "@graphql-codegen/cli": "^2.16.4",
    "@graphql-codegen/client-preset": "^1.2.6",
    "@graphql-codegen/typescript": "^2.8.7",
    "@graphql-codegen/typescript-operations": "^2.5.12",
    "@testing-library/dom": "^8.20.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@tsconfig/recommended": "^1.0.2",
    "@types/jest": "^29.4.0",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "cross-var-no-babel": "^1.2.0",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.0.0",
    "husky": "^8.0.0",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "lint-staged": "^13.1.0",
    "next-sitemap": "^3.1.52",
    "tslint-config-prettier": "^1.18.0",
    "urql": "^3.0.3"
  }
}
